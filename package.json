{
  "name": "@wowvendor/commitlint-config",
  "version": "2.0.0",
  "description": "Commitlint config enforcing conventional commits",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "run-p test:*",
    "test:lint": "eslint .",
    "release": "standard-version",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "author": "weyheyhey <iweyheyhey@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wowvendor/commitlint-config.git"
  },
  "keywords": [
    "conventional-changelog",
    "commitlint-config",
    "commitlint"
  ],
  "dependencies": {
    "@commitlint/config-conventional": "^12.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@wowvendor/eslint-config-base": "^2.4.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.25.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "standard-version": "^9.2.0"
  }
}
